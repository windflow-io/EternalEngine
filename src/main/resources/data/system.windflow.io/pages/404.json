{
    "metaInfo": {
        "title": "${errorTitle}",
        "htmlAttrs" : {
            "lang" : "en"
        },
        "bodyAttrs" : {
            "class" : "dark mode"
        },
        "meta": [
            {"name": "robots", "content": "noindex, follow"}
        ],
        "link": [
            {"rel":"icon" ,"type": "img/png", "href": "https://cdn.windflow.io/app/img/favicon-32x32.png", "sizes": "32x32"},
            {"rel":"icon" ,"type": "img/png", "href": "https://cdn.windflow.io/app/img/favicon-16x16.png", "sizes": "16x16"},
            {"rel":"stylesheet", "href": "https://cdn.windflow.io/app/vendor/tailwindcss/tailwind.min.css"}
        ]
    },
    "layout": "system.windflow.io.CenteredLayout",
    "areas": [
        {
            "area": "center",
            "components":[
                {"name":"system.windflow.io.NotFoundDisplay", "id":"notFoundDisplay"}
            ]
        }],
    "data": {
        "components": {
            "notFoundDisplay": {
                "errorTitle": "${errorTitle}",
                "errorDescription": "${errorDescription}",
                "errorDetail": "${errorDetail}"
            }
        }
    }
}
